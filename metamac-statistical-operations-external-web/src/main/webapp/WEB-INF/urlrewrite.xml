<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
	<rule>
		<from>/apis/operations/latest/</from>
		<to type="redirect">/operations/v1.0/</to>
	</rule>
	<rule>
		<from>/apis/operations/latest</from>
		<to type="redirect">/operations/v1.0/</to>
	</rule>
	<rule>
		<from>^/apis/operations/v1.0$</from>
		<to type="redirect">/operations/v1.0/</to>
	</rule>
</urlrewrite>